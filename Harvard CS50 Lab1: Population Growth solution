#include <stdio.h>
#include <cs50.h>

int main(void)
{
    int start;
    do
    {
        start = get_int("Population size: ");
    }
    while (start < 9);

    int end;
    do
    {
        end = get_int("End population size: ");
    }
    while (end < start);

    int y = 0;
    while(start < end)
    {
        start = start + (start/3) - (start/4);
        y++;
    }
    printf("Years: %i\n", y);
}
